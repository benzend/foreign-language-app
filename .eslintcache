s\" component={LanguageOptions} />\r\n          <Route path=\"/settings\" component={Settings} />\r\n          <Route path=\"/spanish/lesson1\" component={SpanishLesson1} />\r\n          <Route path=\"/spanish/lessons\" component={SpanishLessons} />\r\n          <Route path=\"/german/lesson1\" component={GermanLesson1} />\r\n          <Route path=\"/german/lessons\" component={GermanLessons} />\r\n          <Route path=\"/friends\" component={Friends} />\r\n          <Route path=\"/leaderboards\" component={Leaderboards} />\r\n          <Route path=\"/signout\" component={SignOut} />\r\n          <Route path=\"/\" component={Home} />\r\n        </Switch>\r\n      </Router>\r\n    );\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\12086\\OneDrive\\Desktop\\React_Projects\\language-app-tsx\\frontend\\src\\components\\GermanLessons.tsx",[],"C:\\Users\\12086\\OneDrive\\Desktop\\React_Projects\\language-app-tsx\\frontend\\src\\components\\GermanLesson1.tsx",[],"C:\\Users\\12086\\OneDrive\\Desktop\\React_Projects\\language-app-tsx\\frontend\\src\\components\\Settings.tsx",[],"C:\\Users\\12086\\OneDrive\\Desktop\\React_Projects\\language-app-tsx\\frontend\\src\\components\\SpanishLessons.tsx",[],"C:\\Users\\12086\\OneDrive\\Desktop\\React_Projects\\language-app-tsx\\frontend\\src\\components\\LanguageOptions.tsx",[],"C:\\Users\\12086\\OneDrive\\Desktop\\React_Projects\\language-app-tsx\\frontend\\src\\components\\SpanishLesson1.tsx",[],"C:\\Users\\12086\\OneDrive\\Desktop\\React_Projects\\language-app-tsx\\frontend\\src\\pages\\Friends.tsx",[],"C:\\Users\\12086\\OneDrive\\Desktop\\React_Projects\\language-app-tsx\\frontend\\src\\pages\\Home.tsx",[],"C:\\Users\\12086\\OneDrive\\Desktop\\React_Projects\\language-app-tsx\\frontend\\src\\pages\\SignIn.tsx",["172"],"import { PageFlexCenteredLayout } from \"../layouts/PageFlexCenteredLayout\";\r\nimport { PageTitleLayout } from \"../layouts/PageTitleLayout\";\r\n\r\nimport { Button } from \"../components/Button\";\r\nimport { F